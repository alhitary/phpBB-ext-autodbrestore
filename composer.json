{
    "name": "blitze/autodbrestore",
    "type": "phpbb-extension",
    "description": "Automatically restore database to a specified backup",
    "homepage": "https://github.com/blitze/phpBB-ext-autodbrestore",
    "version": "1.0.0",
    "time": "2017-10-12",
    "license": "GPL-2.0",
    "authors": [{
        "name": "Daniel A.",
        "email": "shizzah@hotmail.com",
        "homepage": "https://github.com/blitze",
		"role": "Lead Developer"
    }],
    "require": {
        "php": ">=5.3.3",
        "composer/installers": "~1.0"
    },
    "extra": {
        "display-name": "Auto Database Restore",
        "soft-require": {
            "phpbb/phpbb": ">=3.2.0,<3.2.0@dev"
        },
		"version-check": {
			"host": "blitze.github.io",
			"directory": "/versions",
			"filename": "autodbrestore.json"
		}
    },
    "require-dev": {
        "phing/phing": "2.4.*"
    }
}
